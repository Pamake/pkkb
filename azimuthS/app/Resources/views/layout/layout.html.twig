<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <title>{% block title %}Smarty - Multipurpose + Admin{% endblock %}</title>
    <meta name="description" content="" />
    <meta name="Author" content="Dorin Grigoras [www.stepofweb.com]" />

    <!-- mobile settings -->
    <meta name="viewport" content="width=device-width, maximum-scale=1, initial-scale=1, user-scalable=0" />
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->

    <!-- WEB FONTS : use %7C instead of | (pipe) -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600%7CRaleway:300,400,500,600,700%7CLato:300,400,400italic,600,700" rel="stylesheet" type="text/css" />


    {% block stylesheets %}

        <!-- CORE CSS -->
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/plugins/bootstrap/css/bootstrap.min.css') }}" />

        <!-- SWIPER SLIDER -->
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/plugins/slider.swiper/dist/css/swiper.min.css') }}" />


        <!-- THEME CSS -->
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/css/essentials.css') }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/css/layout.css') }}" />

        <!-- PAGE LEVEL SCRIPTS -->
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/css/header-1.css') }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset('assets/css/color_scheme/green.css') }}" />

    {% endblock %}




</head>
<body class="smoothscroll enable-animation">

<!-- CONTENU -->
<div id="wrapper">

    <!-- Top Bar -->
    {% include "::moduleUsed/topbar.html.twig" %}

    <!-- HEADER -->
    {% include "::moduleUsed/header.html.twig" %}

    <!-- MAIN
    <div class="ariane">
    </div> -->
        {% block principal %}{% endblock principal %}


    <!-- FOOTER -->
    {% include "::moduleUsed/footer.html.twig" %}
    <!-- /FOOTER -->


</div>



{% block javascripts %}
    <!-- JAVASCRIPT FILES -->
    <script type="text/javascript" src="{{ asset('assets/plugins/jquery/jquery-3.2.1.min.js')}}"></script>

    <script type="text/javascript" src="{{ asset('assets/js/scripts.js')}}"></script>


    <!-- SWIPER SLIDER -->
    <script type="text/javascript" src="{{ asset('assets/plugins/slider.swiper/dist/js/swiper.min.js')}}"></script>

    <script type="text/javascript" src="{{ asset('assets/js/view/demo.swiper_slider.js')}}"></script>
{% endblock %}
</body>
</html>
